{
	"name": "ne10",
	"version": "0.1.0",
	"description": "something neon",
	"main": "dist/main.js",
	"scripts": {
		"dev": "ts-node src/index.ts",
		"dev:watch": "ts-node-dev --respawn src/index.ts",
		"clean": "rimraf dist/*",
		"tsc": "tsc",
		"webpack": "webpack",
		"build": "npm-run-all clean tsc webpack",
		"start": "electron ./dist/main.js --debug",
		"as:mac": "electron-builder --mac --x64",
		"package:mac": "npm-run-all build as:mac",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gsy0911/ne10.git"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/gsy0911/ne10/issues"
	},
	"homepage": "https://github.com/gsy0911/ne10#readme",
	"devDependencies": {
		"@babel/core": "^7.14.5",
		"@types/node": "^15.12.2",
		"@types/react": "^17.0.11",
		"@types/react-dom": "^17.0.7",
		"@types/react-redux": "^7.1.16",
		"@types/react-router-dom": "^5.1.7",
		"autoprefixer": "^9.8.6",
		"babel-loader": "^8.2.2",
		"copy-webpack-plugin": "^9.0.0",
		"electron": "^13.1.2",
		"electron-builder": "^22.10.5",
		"html-webpack-plugin": "^5.3.1",
		"npm-run-all": "^4.1.5",
		"postcss-flexbugs-fixes": "^4.2.1",
		"rimraf": "^2.7.1",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"ts-node-dev": "^1.1.6",
		"typescript": "^4.3.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.2"
	},
	"lint-staged": {
		"*.{js,ts}": "eslint --cache --fix",
		"*.ts": "tsc --noEmit",
		"*.{js,ts,json}": "prettier --write"
	},
	"build": {
		"extends": null,
		"productName": "ne10",
		"appId": "ne10.mdrskn.com",
		"directories": {
			"output": "./products"
		},
		"files": [
			"dist/**/*"
		],
		"dmg": {
			"contents": [
				{
					"x": 410,
					"y": 150,
					"type": "link",
					"path": "/Applications"
				},
				{
					"x": 130,
					"y": 150,
					"type": "file"
				}
			]
		},
		"mac": {
			"icon": "./src/icon/mac_icon.icns",
			"target": [
				"dmg"
			]
		}
	},
	"dependencies": {
		"@headlessui/react": "^1.2.0",
		"@material-ui/core": "^4.11.4",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-redux": "^7.2.4",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0",
		"redux": "^4.1.0",
		"reset-css": "^5.0.1"
	}
}
